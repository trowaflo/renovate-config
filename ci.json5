{
  packageRules: [
    {
      "matchManagers": ["github-actions"],
      "matchUpdateTypes": ["patch", "minor"],
      "matchPackageNames": ["actions/checkout"],
      "groupName": "AUTOMERGE - GitHub Actions",
      "commitMessageAction": "ci(github-actions):",
      "commitMessageTopic": "{{depName}}",
      "automerge": true
    }
  ]
}
